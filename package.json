{
	"name": "starter-template",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"format": "prettier --write ./src/**/* --config ./.prettierrc.js",
		"lint": "eslint ./src/**/* --ext .tsx,.ts,.js,.jsx --fix"
	},
	"dependencies": {
		"@headlessui/react": "^1.4.0",
		"@heroicons/react": "^1.0.3",
		"@react-hook/window-size": "^3.0.7",
		"@types/react-slick": "^0.23.5",
		"@types/react-swipeable-views": "^0.13.1",
		"autoprefixer": "^10.3.1",
		"clsx": "^1.1.1",
		"next": "^11.1.0",
		"postcss": "^8.3.6",
		"react": "^17.0.2",
		"react-awesome-reveal": "^3.8.1",
		"react-dom": "17.0.2",
		"react-responsive-carousel": "^3.2.20",
		"react-rough-notation": "^1.0.1",
		"react-slick": "^0.28.1",
		"react-swipeable": "^6.2.0",
		"react-swipeable-views": "^0.14.0",
		"slick-carousel": "^1.8.1",
		"tailwindcss": "^2.2.7",
		"typescript-eslint": "^0.0.1-alpha.0"
	},
	"devDependencies": {
		"@types/css-font-loading-module": "^0.0.6",
		"@types/next": "^9.0.0",
		"@types/react": "^17.0.17",
		"@typescript-eslint/eslint-plugin": "^4.29.1",
		"@typescript-eslint/parser": "^4.29.1",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript-prettier": "^4.2.0",
		"eslint-config-next": "^11.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-plugin-import": "^2.24.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"typescript": "^4.3.5"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"./src/**/*": [
			"eslint ./src/**/* --ext .tsx,.ts,.js,.jsx --fix",
			"prettier --write ./src/**/* --config ./.prettierrc.js"
		],
		"*.json": [
			"prettier --write"
		]
	},
	"packageManager": "yarn@3.0.1"
}
